        "medtechchain": {
            "peers": {
                "peer0.medtechchain.nl": {},
                "peer0.medivale.nl": {}
            },
            "connection": {
                "timeout": {
                    "peer": {
                        "endorser": "6000",
                        "eventHub": "6000",
                        "eventReg": "6000"
                    }
                }
            }
        }
    },
    "organizations": {
        "MedTechChainPeerMSP": {
            "mspid": "MedTechChainPeerMSP",
